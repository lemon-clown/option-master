{
  "name": "option-master",
  "version": "0.0.17",
  "author": {
    "name": "lemon-clown",
    "url": "https://github.com/lemon-clown/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemon-clown/option-master"
  },
  "keywords": [
    "json-schema",
    "config util",
    "option util"
  ],
  "description": "option utils",
  "main": "lib/index.js",
  "license": "MIT",
  "engines": {
    "node": ">= 10.16.1"
  },
  "scripts": {
    "clean": "rimraf test/**/*.output.json test/**/*.outputs.json",
    "start": "tsc -b tsconfig.json --watch",
    "build": "rimraf lib/ && tsc -b tsconfig.json",
    "postbuild": "rimraf lib/package.json lib/test && mv lib/src/* lib && rimraf lib/src/ lib/**/*.tsbuildinfo",
    "lint:fix": "eslint src/**/*.ts --fix",
    "answer": "cross-env node -r ts-node/register test/answer.ts",
    "test": "mocha -r ts-node/register test/**/*.test.ts",
    "test:file": "mocha -r ts-node/register",
    "prepublishOnly": "yarn lint:fix && yarn test && yarn build"
  },
  "peerDependencies": {
    "change-case": "^4.1.1"
  },
  "devDependencies": {
    "@lemon-clown/mocha-test-master": "^0.0.1",
    "@types/chai": "^4.2.4",
    "@types/fs-extra": "^8.0.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.5",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "chai": "^4.2.0",
    "chai-exclude": "^2.0.2",
    "change-case": "^4.1.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "fs-extra": "^8.1.0",
    "mocha": "^6.2.2",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.4"
  }
}
